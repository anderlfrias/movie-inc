{
  "expo": {
    "name": "Movies Inc.",
    "slug": "movie-inc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/movies-inc-logo512.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "movie-inc",
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anderlfrias.movieinc"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c846f9fe-634b-47f2-b8b3-21c63ef13dce"
      }
    },
    "owner": "anderlfrias"
  }
}